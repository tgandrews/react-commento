  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "np": "^6.2.3",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "husky": {
